<odoo>
    <data>
        <record id="fleet_vehicle_type_form" model="ir.ui.view">
            <field name="name">fleet.vehicle.type.form</field>
            <field name="model">fleet.vehicle.type</field>
            <field name="arch" type="xml">
                <form string="fee">
                    <sheet>
                        <group col="2">
                            <group>
                                <field name="name"/>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="status"/>
                                <field name="description"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="fleet_vehicle_type_tree" model="ir.ui.view">
            <field name="name">fleet.vehicle.type.tree</field>
            <field name="model">fleet.vehicle.type</field>
            <field name="arch" type="xml">
                <form string="fee">

                </form>
            </field>
        </record>
        <record id="fleet_vehicle_type_action" model="ir.actions.act_window">
            <field name="name">Vehicle type</field>
            <field name="res_model">fleet.vehicle.type</field>
            <field name="view_mode">tree,kanban,form</field>
        </record>
    </data>
</odoo>