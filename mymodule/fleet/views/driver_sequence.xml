<odoo>
    <data noupdate="1">
        <record id="sequence_driver" model="ir.sequence">
            <field name="name">Driver Sequence</field>
            <field name="code">self.fleet.driver</field>
            <field name="active">TRUE</field>
            <field name="prefix">DRKK</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
        <record id="sequence_fleet_driver_equipment_part" model="ir.sequence">
            <field name="name">Driver equipment part</field>
            <field name="code">self.fleet.driver.equipment.part</field>
            <field name="active">TRUE</field>
            <field name="prefix">DRE</field>
            <field name="padding">6</field>
        </record>
    </data>
</odoo>