<odoo>
    <data>


        <record id='warehouse_view' model='ir.actions.act_window'>
            <field name="name">Warehouse</field>
            <field name="res_model">sharevan.warehouse</field>
            <field name="view_mode">map,tree,kanban,form</field>
            <field name="context">{"search_default_status" : 'running'}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new warehouse
                </p>
                <p>
                    Here you can add warehouse
                </p>
            </field>
        </record>


        <record id='depot_view' model='ir.actions.act_window'>
            <field name="name">Depot</field>
            <field name="res_model">sharevan.depot</field>
            <field name="view_mode">map,tree,kanban,form</field>
            <field name="context">{"search_default_status" : 'running'}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a new depot
                </p>
                <p>
                    Here you can add depot
                </p>
            </field>
        </record>

    </data>
</odoo>